  "description": "h5-wing",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development&&node scripts/build.js",
    "start": "npm run dev server",
    "build": "npm rebuild node-sass && cross-env NODE_ENV=production ./scripts/build.js --progress --bail",
    "config": "jar cvf temp.zip ./META-INF/ && java -jar autoconfig -I -u config.properties temp.zip && unzip temp.zip -d temp && mv ./temp/autoconfig.js ./ && rm -rf temp.zip temp",
    "test": "cross-env NODE_ENV=test eslint src/**/*.js && jest --watch",
    "testEslint": "cross-env NODE_ENV=test eslint src/__tests__/**/*.js src/__mocks__/**/*.js",
    "testUpdate": "cross-env NODE_ENV=test eslint src/__tests__/**/*.js src/__mocks__/**/*.js && jest --updateSnapshot",
    "coverage": "cross-env NODE_ENV=test jest --updateSnapshot --coverage",
    "sonar": "cross-env NODE_ENV=test jest --updateSnapshot --coverage; node src/__tests__/sonar.js"
  },
  "repository": {
    "type": "git"
  },
